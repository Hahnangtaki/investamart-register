relationship OneToOne {
	AccountIndividuRegister to AccountRegister
}

relationship OneToOne {
	AccountInstitutionRegister to AccountRegister
}

relationship OneToMany {
	AccountRegister to AccountAuthorizeRegister
}

relationship OneToMany {
	AccountRegister to InvestorAddressRegister
}

relationship OneToOne {
	Visitor to VisitorRegisterAccount
}

relationship OneToMany {
	Visitor to OtpHistory
}

paginate InvestorAddressRegister with pagination

dto * with mapstruct

service all with serviceImpl
