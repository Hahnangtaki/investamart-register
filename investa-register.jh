entity OtpHistory {
	createdBy Integer,
    otpCreatedDate ZonedDateTime,
    otpExpiredDate ZonedDateTime,
    memberEmail String,
    phoneNumber String,
    procType Integer,
    tokenText String,
    token String,
    retryCount Integer,
    retryMax Integer,
    requestData Blob,
    
    createSystemDate LocalDate,
    createDate ZonedDateTime,
    createUserId Long,
    lastModificationSystemDate LocalDate,
    lastModificationDate ZonedDateTime,
    lastModificationUserId Long
}

entity Visitor {
	email String maxlength(100),
    password String maxlength(100),
    mobilePhone String,
    memberSince LocalDate,
    memberStatus Integer,
    
    createSystemDate LocalDate,
